    "eslint-config-love": "^47.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "patch-package": "^8.0.0",
    "ts-jest": "^29.1.2"
  },
  "keywords": [
    "base16",
    "tinted-theming",
    "color-schemes",
